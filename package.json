{
  "name": "time-management-matrix-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "bun@1.1.38",  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:migrate": "turbo db:migrate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },  "devDependencies": {
    "turbo": "^2.3.2",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
